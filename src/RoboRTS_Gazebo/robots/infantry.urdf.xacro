  <link
    name="br_wheel_link">
    <inertial>
      <origin
        xyz="0 0.014616 0"
        rpy="0 0 0" />
      <mass
        value="0.86078" />
      <inertia
        ixx="0.0015361"
        ixy="1.6456E-06"
        ixz="-2.0131E-06"
        iyy="0.002844"
        iyz="-9.5351E-07"
        izz="0.0015337" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://roborts_gazebo/meshes/br_wheel_link.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="1 0 0 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="1.5708 0 0" />
      <geometry>
        <cylinder
          radius = "${wheel_radius}"
          length = "${wheel_width}" />
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0.0</mu>
            <mu2>0.0</mu2>
          </ode>
        </friction>
      </surface>
    </collision>
  </link>
